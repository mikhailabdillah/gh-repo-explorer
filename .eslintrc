{
  "root": true,
  "plugins": ["react", "prettier", "react-hooks", "jsx-a11y"],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "rules": {
    "react/react-in-jsx-scope": 0,
    "jsx-a11y/rule-name": 0,
    "no-console": 2
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
